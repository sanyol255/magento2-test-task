<?xml version="1.0" encoding="UTF-8" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="SportStore_CustomerFeedback::feedback" translate="title"
                title="Customers Feedbacks" sortOrder="30">
                    <resource id="SportStore_CustomerFeedback::feedback_grid" translate="title"
                    title="List of Customers Feedbacks" sortOrder="10" />
                    <resource id="SportStore_CustomerFeedback::feedback_details" translate="title"
                    title="Feedback Details" sortOrder="20" />
                    <resource id="SportStore_CustomerFeedback::feedback_manage" translate="title"
                    title="Manage Customer Feedback" sortOrder="30" />
                    <resource id="SportStore_CustomerFeedback::feedback_delete" translate="title"
                              title="Delete feedback" sortOrder="40" />
<!--                    <resource id="SportStore_CustomerFeedback::feedback_status" translate="title"-->
<!--                              title="Change feedback status" sortOrder="50" />-->
                    <resource id="SportStore_CustomerFeedback::feedback_save" translate="title"
                              title="Save customer feedback" sortOrder="60" />
                </resource>
            </resource>
        </resources>
    </acl>
</config>
